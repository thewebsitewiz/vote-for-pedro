sub getNumber {
  my ($max,$min) = @_;

  $min = 1 unless $min;

  my $number = 0;

  unless ($max) {
    die("Max limit missing!");
  }

  if ($max && $min) {
    $number = int(rand($max)) + $min;
  }

  return $number;


}

sub getWords {
  my ($type) = @_;

  # title caption summary shortSummary body
  die("No type provided!") unless $type;

  my $sentences = 0;
  my $paragraphs = 1;
  my $words = 0;
  my $punctuation = 0;

  if ($type eq q[title] || $type eq q[caption]) {
    $sentences = 1;
    $words = getNumber(18,6);
    $punctuation = 0;
  }
  elsif ($type eq q[preTitle] || $type eq q[subTitle]) {
    $sentences = 1;
    $words = getNumber(14,6);
    $punctuation = 0;
  }
  elsif ($type eq q[summary]) {
    $sentences = getNumber(4,2);
    $words = getNumber(12,6);
    $punctuation = 1;
  }
  elsif ($type eq q[shortSummary]) {
    $sentences = getNumber(3,2);
    $words = getNumber(8,6);
    $punctuation = 1;
  }
  elsif ($type eq q[body]) {
    $paragraphs = getNumber(2,1);
    $words = getNumber(12,6);
    $punctuation = 1;
  }
  else {
    die("Invalid type provided!");
  }

  my @paragraphs = ();
  foreach my $p (1..$paragraphs) {
    $sentences = getNumber(10,4) unless $sentences;
    my @paragraph = ();
    my @sent = ();
    my $sentence = q[];
    foreach my $s (1..$sentences) {
      my @words = ();
      foreach my $w (1..$words) {
	my $word = getWord();
	if ($w == 1) {
	  $word = ucfirst($word);
	}
	push @words,$word;
      }
      $sentence = join' ',@words;
      $sentence = qq[$sentence.] if $punctuation;
      push @paragraph,$sentence;
    }
    my $parag = join' ',@paragraph;
    push @paragraphs,$parag;
  }
  my $paragr = join qq[\n],@paragraphs;

}



sub getWord {
  return $wordList[int(rand(2000))];
}

