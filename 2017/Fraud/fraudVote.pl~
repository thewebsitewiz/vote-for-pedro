use DBI;





my $delete_rfq = qq[delete from [$targetDB].[dbo].[RegistrationFacilities]];
$ddbh->do($delete_rfq);






my @wordList = getWordList();

my @sql = ();

my @tables = qw(articleBeats articleTopics articles rolePrivileges privileges userRoles roles users beats topics);

# clear tables
foreach my $table (@tables) {
  push @sql,qq[delete from $table;];
}


# create privileges
my @privs = qw(Create-Own-Article
	       Create-Others-Article
	       Create-Staff-Article
	       Edit-Own-Article
	       Edit-Others-Article
	       Approve-Own-Article
	       Approve-Others-Article
	       Add-New-User
	       Edit-Own-User
	       Edit-Other-User
	       Assign-Author-Roles
	       Assign-Editor-Roles
	       Assign-Any-Role
	       Assign-Any-Privilege
	       Enter-Any-Event
	       Enter-Venue-Event
	       Edit-Any-Event
	       Edit-Venue-Event
	       Approve-Own-Event
	       Approve-Any-Event
	    );

foreach my $priv (@privs) {
  push @sql,qq[INSERT INTO privileges (privilegeName) VALUES ('$priv');];
}

# create roles
my @roles = qw(Author
	       Senior-Author
	       Editor
	       Senior-Editor
	       Managing-Editor
	       Webmaster
	       Venue-Admin
	       Event-Entry
	       Event-Admin
	       MB-Intern
	       MB-Employee
	       MB-Manager
	     );

foreach my $role (@roles) {
  push @sql,qq[INSERT INTO roles (roleName) VALUES ('$role');];
}

# associate privileges with role

my %rP = (
	  'Author'          => ['Create-Own-Article',
				'Create-Staff-Article',
				'Edit-Own-Article',
			       ],
	  'Senior-Author'   => [
				'Create-Own-Article',
